    "coverage": "jest --coverage",
    "test-watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/morungos/node-secure-stream.git"
  },
  "dependencies": {},
  "devDependencies": {
    "concat-stream": "^1.6.0",
    "jest": "^26.6.0"
  },
  "author": "Stuart Watt <stuart@morungos.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/morungos/node-secure-stream/issues"
  },
  "homepage": "https://github.com/morungos/node-secure-stream#readme",
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testRegex": "(/__tests__/.*)test\\.js$",
    "collectCoverageFrom": [
      "lib/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
